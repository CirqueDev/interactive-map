{
  "name": "@cirquedev/interactive-map",
  "version": "1.0.1",
  "description": "A map component to visualise our touring shows",
  "files": [
    "dist"
  ],
  "main": "./dist/interactive-map.umd.js",
  "module": "./dist/interactive-map.es.js",
  "exports": {
    ".": {
      "import": "./dist/interactive-map.es.js",
      "require": "./dist/interactive-map.umd.js"
    }
