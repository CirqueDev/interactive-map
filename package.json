    "build": "vite build",
    "build:staging": "vite build --mode staging",
    "serve": "vite preview",
    "test": "jest --verbose"
  },
  "dependencies": {
    "@googlemaps/js-api-loader": "^1.12.4",
    "@googlemaps/markerclusterer": "^2.0.8",
