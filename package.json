    "vue": "^2.7.0"
  },
  "devDependencies": {
    "@babel/core": "^7.15.8",
    "@babel/preset-env": "^7.15.8",
    "@vitejs/plugin-vue2": "^1.1.2",
    "@vue/compiler-sfc": "^3.2.20",
    "@vue/test-utils": "^1.2.2",
    "babel-jest": "^26.6.3",
    "dotenv": "^16.0.1",
    "flush-promises": "^1.0.2",
    "jest": "^26.6.3",
    "sass": "^1.43.2",
    "vite": "^2.6.4",
    "vue-jest": "^4.0.1",
    "vue-template-compiler": "^2.7.0"
  }
